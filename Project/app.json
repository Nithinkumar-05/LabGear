{
  "expo": {
    "name": "Lab Gear",
    "slug": "Project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lab_gear.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lab_gear.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nithineruventi.LabGear"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/lab_gear.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lab_gear.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8cc03df-7575-4e49-bb68-4ffff9370a41"
      }
    }
  }
}
